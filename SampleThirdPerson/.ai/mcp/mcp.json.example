{
  "mcpServers": {
    "ue_editor_mcp_dev": {
      "command": "uvx",
      "args": [
        "--from",
        "your-path-here",
        "unreal-editor-mcp"
      ]
    },
    "serena": {
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--context",
        "ide-assistant",
        "--enable-web-dashboard",
        "false"
      ]
    }
  }
}